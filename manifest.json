{
  "manifest_version": 3,
  "name": "Easy Keys",
  "version": "1.0.0",
  "description": "Clavier virtuel pour caractères accentués - Virtual keyboard for accented characters",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Easy Keys - Caractères accentués"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "clipboardWrite"
  ]
}
